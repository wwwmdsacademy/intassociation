current = 0;\n      var itemsPerView = 2;\n      var gap = 20; // approximates 1.25rem\n\n      function getItemsPerView() {\n        var width = window.innerWidth || document.documentElement.clientWidth || 1024;\n        if (width <= 992) return 1;\n        return 2;\n      }\n\n      function layout(){\n        if (!viewport || !track) return;\n        itemsPerView = getItemsPerView();\n        var vw = viewport.clientWidth;\n        var totalGap = gap * (itemsPerView - 1);\n        var cardWidth = Math.max(220, Math.floor((vw - totalGap) / itemsPerView));\n        cards.forEach(function(card){ card.style.width = cardWidth + 'px'; });\n\n        var maxIndex = Math.max(0, cards.length - itemsPerView);\n        if (current > maxIndex) current = maxIndex;\n\n        buildDots();\n        translate();\n        updateNav();\n      }\n\n      function translate(){\n        if (!track || !viewport) return;\n        var first = cards[0];\n        var cardWidth = first ? first.getBoundingClientRect().width : 0;\n        var offset = current * (cardWidth + gap);\n        track.style.transform = 'translateX(' + (-offset) + 'px)';\n        setActiveDot(current);\n      }\n\n      function updateNav(){\n        var maxIndex = Math.max(0, cards.length - itemsPerView);\n        if (prevBtn) prevBtn.disabled = current <= 0;\n        if (nextBtn) nextBtn.disabled = current >= maxIndex;\n      }\n\n      function next(){\n        var maxIndex = Math.max(0, cards.length - itemsPerView);\n        if (current < maxIndex) { current++; translate(); updateNav(); }\n      }\n      function prev(){\n        if (current > 0) { current--; translate(); updateNav(); }\n      }\n\n      function buildDots(){\n        if (!dotsWrap) return;\n        dotsWrap.innerHTML = '';\n        var maxIndex = Math.max(0, cards.length - itemsPerView);\n        for (var i = 0; i <= maxIndex; i++){\n          var btn = document.createElement('button');\n          btn.type = 'button';\n          btn.className = 'dot' + (i === current ? ' is-active' : '');\n          btn.setAttribute('aria-label', 'Перейти к отзывам ' + (i + 1));\n          if (i === current) btn.setAttribute('aria-current', 'true');\n          (function(index){\n            btn.addEventListener('click', function(){\n              current = index;\n              translate();\n              updateNav();\n            });\n          })(i);\n          dotsWrap.appendChild(btn);\n        }\n      }\n      function setActiveDot(index){\n        if (!dotsWrap) return;\n        var dots = dotsWrap.querySelectorAll('.dot');\n        dots.forEach(function(d, i){\n          if (i === index) { d.classList.add('is-active'); d.setAttribute('aria-current', 'true'); }\n          else { d.classList.remove('is-active'); d.removeAttribute('aria-current'); }\n        });\n      }\n\n      if (prevBtn) prevBtn.addEventListener('click', prev);\n      if (nextBtn) nextBtn.addEventListener('click', next);\n\n      // Keyboard navigation\n      if (viewport) {\n        viewport.addEventListener('keydown', function(e){\n          if (e.key === 'ArrowLeft') prev();\n          if (e.key === 'ArrowRight') next();\n        });\n      }\n\n      window.addEventListener('resize', layout);\n      setTimeout(layout, 0);\n    })();\n  "}]}]},{"tagName":"section","classes":["gjs-t-body"],"attributes":{"id":"cta-join-today","aria-label":"Призыв к действию (CTA)"},"components":[{"classes":["inner"],"components":[{"tagName":"h2","type":"heading","classes":["gjs-t-h1","fade-seq","fade-1"],"components":[{"type":"textnode","content":"Вступайте в наше сообщество уже сегодня"}]},{"classes":["cta","fade-seq","fade-2"],"components":[{"type":"link","editable":false,"classes":["gjs-t-button","btn","btn-accent"],"attributes":{"href":"https://wa.me/?text=%D0%97%D0%B4%D1%80%D0%B0%D0%B2%D1%81%D1%82%D0%B2%D1%83%D0%B9%D1%82%D0%B5!%20%D0%A5%D0%BE%D1%87%D1%83%20%D0%B2%D1%81%D1%82%D1%83%D0%BF%D0%B8%D1%82%D1%8C%20%D0%B2%20%D0%BC%D0%B5%D0%B6%D0%B4%D1%83%D0%BD%D0%